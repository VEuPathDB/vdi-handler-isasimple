!/usr/bin/env bash

# THIS SCRIPT NEEDS TO BE UPGRADED TO ALLOW BOTH INSTALL AND UPDATE

# exit when any command fails
set -e

userDatasetId=$1
metadataFile=$2

if [ -z $userDatasetId ] || [ -z $metadataFile ]; then
    echo "Usage: install-data user_dataset_id meta_json_file."
    exit 255
fi

makeGusConfigFile temp_gus.config

export NLS_LANG='AMERICAN_AMERICA.UTF8';

 ga ApiCommonData::Load::Plugin::InsertUserDatasetAttributes \
     --userDatasetId $userDatasetId \
     --metadataFile $metadataFile \
     --commit
     --gusconfigfile temp_gus.config

 

